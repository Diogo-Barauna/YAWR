
{
        "manifest_version": 3,
        "name": "Yet Another Water Reminder",
        "version":"4.3.2",
        "description":"Um lembrete para beber água! Água é vida!",
        "action": {
            "default_popup": "popup.html",
            "default_icon": {
              "16": "icons/16.png",
              "32": "icons/32.png",
              "48": "icons/48.png",
              "128": "icons/128.png"
            }
          },
          "icons": {
            "16": "icons/16.png",
            "32": "icons/32.png",
            "48": "icons/48.png",
            "128": "icons/128.png"
          },
        "background":{
            "service_worker":"background.js"
        },
        "permissions":[
            "alarms",
            "notifications",
            "storage",
            "background"
        ]
    }
    